<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="findAllUser">
        SELECT
        *
        FROM TBL_ALLUSER;
    </entry>
    <entry key="findAllCategory">
        SELECT
            CATEGORY_CODE
          FROM TBL_CATEGORY
    </entry>
    <entry key="insertMenu">
        INSERT INTO TBL_MENU(
            MENU_CODE,
            MENU_NAME,
            MENU_PRICE,
            CATEGORY_CODE,
            ORDERABLE_STATUS
        ) VALUES(
            NULL,
            ?,
            ?,
            ?,
            ?
        )
    </entry>
</properties>